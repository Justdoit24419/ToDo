{
  "version": 1,
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/github",
        "--key",
        "${GITHUB_MCP_KEY}",
        "--profile",
        "${GITHUB_MCP_PROFILE}"
      ],
      "disabled": true
    },
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@wonderwhy-er/desktop-commander",
        "--key",
        "${DESKTOP_COMMANDER_MCP_KEY}",
        "--profile",
        "${DESKTOP_COMMANDER_MCP_PROFILE}"
      ],
      "disabled": true
    },
    "playwright-stealth": {
      "command": "/opt/homebrew/bin/node",
      "args": [
        "/opt/homebrew/lib/node_modules/@pvinis/playwright-stealth-mcp-server/dist/index.js"
      ],
      "disabled": true
    },
    "n8n-mcp": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "MCP_MODE=stdio",
        "-e",
        "LOG_LEVEL=error",
        "-e",
        "DISABLE_CONSOLE_OUTPUT=true",
        "-e",
        "N8N_API_URL=${N8N_API_URL}",
        "-e",
        "N8N_API_KEY=${N8N_API_KEY}",
        "ghcr.io/czlonkowski/n8n-mcp:latest"
      ],
      "disabled": true
    },
    "filesystem": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-fs"],
      "disabled": true
    }
  },
  "resources": [],
  "tools": []
}
